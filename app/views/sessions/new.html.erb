<header id="header">
  <div class="page">
    <h1>ps2</h1>
  </div>
</header>


<div class="page page-login">
<h2>Log in</h2>
  <%= form_tag sessions_path do %>
    <div class="field">
      <%= label_tag :email %>
      <%= text_field_tag :email, params[:email] %>
    </div>
    <div class="field">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class="actions"><%= submit_tag "Log in" %></div>
  <% end %>
</div>